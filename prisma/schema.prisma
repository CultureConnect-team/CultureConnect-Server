generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String  @id @default(uuid())
  name     String
  email    String  @unique
  password String
  createdAt DateTime @default(now())
}

model Destination {
  id          String  @id @default(uuid())
  name        String
  description String
  location    String
  imageUrl    String?
  createdAt   DateTime @default(now())
}
